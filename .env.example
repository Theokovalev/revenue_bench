# Revenue Bench Environment Variables
# Copy this file to .env and fill in your API keys

# Required: OpenRouter API Key
# Get yours at: https://openrouter.ai/keys
OPENROUTER_API_KEY=sk-or-v1-your-key-here

# Required: Tavily API Key for web search
# Get yours at: https://tavily.com (free tier available)
TAVILY_API_KEY=tvly-your-key-here

# Optional: Model Configuration
DEFAULT_MODEL=openrouter/openai/gpt-5
DEFAULT_TEMPERATURE=0.7
MAX_RETRIES=3

# Optional: Output Configuration
OUTPUT_DIR=results
DEBUG_MODE=false

# Optional: Cost Limits
MAX_COST_PER_EVAL=5.00
WARN_COST_THRESHOLD=2.00
EOF < /dev/null